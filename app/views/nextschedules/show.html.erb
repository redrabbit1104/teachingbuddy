<div class="edit_title">
  <h1>Show Schedule</h1>
</div>

<div class="edit_form_wrapper">

  <div class="input-wrapper">
    <span class="input-subject">Subject</span><%= @set_schedule.subject %>
  </div>
  <span class="input-subject date">Date</span><%= @set_schedule.sdate.sdate %>
  <div class="input-wrapper">
    <div class="schedule-input-time">
      <span class="input-start">Start Time</span><span
        id="edit_start_time"><%= @set_schedule.start_time.to_s(:time) %></span>
    </div>
  </div>
  <div class="input-wrapper">
    <div class="schedule-input-time">
      <span class="input-end">End Time</span><span id="edit_end_time"><%= @set_schedule.end_time.to_s(:time) %></span>
    </div>
  </div>

  <%= form_with model: [@set_schedule, @set_check], class: 'schedule_form',local: true do |f| %>
  <div class="edit_delete_wrapper">
    <%= f.check_box :check, :as => :boolean,checked:true %>
    <%= f.submit "Entry" ,class:"schedule-input",id:"submit-btn"%>
  </div>
  <% end %>

</div>

<%= link_to '戻る' ,nextschedules_path,id:"back" %>

<div>Checked Teachers</div>
<% @get_checked_schedule_users.each do |checked_schedule_user| %>
<%= image_tag(User.find(checked_schedule_user).avatar.variant(resize: '30x30' ))%>
<%= User.find(checked_schedule_user).last_name %>
<%= User.find(checked_schedule_user).first_name %>&nbsp;&nbsp;先生<br>
<% end %>

